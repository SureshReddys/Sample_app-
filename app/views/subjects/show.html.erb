
<%= link_to("<<Back to index",{:action=>:index}, :class=>"action index") %>

 <div class ="subjects show" >
  	<h2>Show pages</h2>

   <table summary="subjects detail view ">

    	<tr>
    		<th>Name</th>
    		<td><%= @subject.name %></td>
    	</tr>

    	<tr>
    		<th>Position</th>
    		<td><%= @subject.position %></td>
         </tr>
         <tr>
         	<th>Visible?</th>
         	<td><%= @subject.visible ? 'true' : 'false' %> </td>
         </tr>
         <tr>
         	 <th>Created_at</th>
         	 <td><%= @subject.created_at %></td>
         </tr>
         <tr>
         	<th>Updated_at</th>
         	<td><%= @subject.updated_at %></td>
         </tr>
   </table>
</div>